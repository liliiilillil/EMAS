{
  "name": "@terminus/rn-template",
  "version": "3.0.36",
  "private": false,
  "scripts": {
    "prepublish": "cp -r ./package-lock.json ./packagelock.json && cp ./.gitignore ./gitignore",
    "wechat:watch": "export NODE_ENV=development && octopus build -w --progress --compress",
    "wechat:build": "octopus build --compress --zip",
    "alipay:watch": "export NODE_ENV=development && octopus build --target alipay -w --progress --compress",
    "alipay:build": "octopus build --target alipay --compress --zip",
    "dingtalk:watch": "octopus build --target dingding -w --progress --compress",
    "dingtalk:build": "octopus build --target dingding --compress --zip",
    "start": "node node_modules/react-native/local-cli/cli.js start --reset-cache",
    "clean-start": "watchman watch-del-all && rm -rf $TMPDIR/react-native-packager-cache-* && rm -rf $TMPDIR/metro-bundler-cache-* && npm cache clean --force && npm start -- --reset-cache",
    "dev": "NODE_ENV=development node ./scripts/dev.js",
    "predev": "node ./scripts/clean.js",
    "ios": "react-native run-ios --simulator \"iPhone 11\"",
    "androidx": "npx jetify",
    "android": "npm run androidx && cd android && ./gradlew clean installDebug -DENV_SWITCH=1",
    "android:win": "cd android && gradlew.bat clean installDebug -DENV_SWITCH=1",
    "androidRelease:win": "cd android && gradlew.bat clean assembleRelease",
    "androidRelease": "cd android && ./gradlew clean assembleRelease",
    "build": "node ./scripts/build.js",
    "server": "herd dicefile-docker.js",
    "precommit": "lint-staged",
    "lint": "./node_modules/.bin/eslint src --cache --ext es6 --ext jsx --ext js --ignore-pattern .gitignore",
    "android-clean": "sed -i '' 's/:@terminus\\//:/' android/settings.gradle && sed -i '' 's/:@terminus\\//:/' android/app/build.gradle && cd android && ./gradlew clean"
  },
  "dependencies": {
    "@react-native-community/netinfo": "^5.6.2",
    "@react-navigation/web": "^1.0.0-alpha.9",
    "@terminus/eevee-config-plugin": "0.0.12",
    "@terminus/mall-base": "^1.0.4",
    "@terminus/mall-utils": "^1.0.21",
    "@terminus/nusi-mobile": "^1.0.2",
    "@terminus/octopus-adapter-alipay": "^4.0.4",
    "@terminus/octopus-adapter-dingding": "^4.0.0",
    "@terminus/octopus-adapter-wx": "^4.0.4",
    "@terminus/octopus-core": "^4.0.0",
    "@terminus/octopus-hooks": "^1.0.3",
    "@terminus/octopus-router": "^3.0.1",
    "@terminus/octopus-shared": "^4.0.0",
    "@terminus/react-native-octopus": "4.0.8",
    "@terminus/react-native-camera": "2.0.1",
    "@terminus/react-native-cookies": "3.3.8",
    "@terminus/react-native-screen-tool": "0.0.4",
    "@terminus/react-native-web": "^0.11.7-terminus-1",
    "@terminus/react-navigation": "4.0.2",
    "@terminus/rn-debug": "1.0.6",
    "@terminus/react-native-image-picker": "2.0.2",
    "@terminus/react-native-emas": "./..",
    "autoprefixer": "^9.7.5",
    "babel-plugin-import": "^1.13.0",
    "koa-send": "^5.0.0",
    "less": "^3.11.1",
    "metro-react-native-babel-preset": "^0.59.0",
    "props-overrider": "0.0.3",
    "react": "16.13.1",
    "react-dom": "^16.13.1",
    "react-native": "0.61.5",
    "react-native-extended-stylesheet": "^0.12.0",
    "react-native-screens": "^2.4.0",
    "react-native-svg": "^12.0.3",
    "react-native-webview": "^9.0.1",
    "react-navigation-redux-helpers": "^4.0.1",
    "react-navigation-tabs": "^2.8.7",
