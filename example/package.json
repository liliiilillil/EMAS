    "node-sass": "^4.13.1",
    "postcss-flexibility": "^2.0.0",
    "postcss-loader": "^3.0.0",
    "react-imageview": "^1.2.1",
    "react-native-image-zoom-viewer": "^2.2.27",
    "react-native-iphone-x-helper": "^1.2.1",
    "react-native-reanimated": "^1.7.1",
    "sass-loader": "^8.0.2",
    "ts-loader": "^6.2.2",
    "tslib": "^1.10.0",
    "url-loader": "^4.0.0",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-merge": "^4.2.2",
