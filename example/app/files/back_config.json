{
  "services": {
     "login": {
        "type": "http",
        "url": "/api/user/login",
        "query": ["username", "password"]
     },
     "getUserById": {
        "type": "http",
        "url": "/api/user/{userId}"
     },
     "getRolesByUserId": {
        "type": "http",
        "url": "/api/user/{userId}/roles"
     },
     "getMemberIdByUserId": {
        "type": "http",
        "url": "/api/member/profile/user/{userId}"
     },
     "getArticleById": {
        "type": "http",
        "url": "/api/article-manage",
        "query": ["id"]
     },
    "getThirdInfoQQ": {
      "method": "GET",
      "type": "http",
      "url": "/api/usercenter/mobile/qq/userInfo",
      "query": ["openId", "accessToken"]
    },
    "getUserIdByCodeMobile": {
      "method": "GET",
      "type": "http",
      "url": "/api/usercenter/mobile/token",
      "query": ["code"]
    },
    "getAssociatedNewAccount": {
      "method": "POST",
      "type": "http",
      "url": "/api/usercenter/mobile/login-by-phone",
      "query": ["phone_number","verification_code","password"]
    },
    "getAssociatedOwnAccount": {
      "method": "POST",
      "type": "http",
      "url": "/api/usercenter/mobile/login",
      "query": ["phone_number","verification_code"]
    },
    "getShopInfo": {
      "method": "GET",
      "type": "http",
      "url": "/api/shop/{id}"
    },
    "getShopCollectState": {
      "method": "GET",
      "type": "http",
      "url": "/api/favorite-shop/check/{shopId}"
    },
    "searchWithAggs": {
      "method": "GET",
      "type": "http",
      "url": "/api/search-in-shop",
      "query": ["q", "bid", "shopId", "attrs", "fcid", "bids", "regionIds", "ids", "bcids", "p_f", "aggs", "sort", "highlight", "pageNo", "pageSize", "shopCatId"]
    },
    "getArticles": {
      "method": "GET",
      "type": "http",
      "url": "/api/article-center/get-by-string-ids",
      "query": ["ids"]
    },
    "getSkuItems": {
      "method": "GET",
      "type": "http",
      "url": "/api/items/item-with-sku",
      "query": ["itemIds"]
    },
    "getConfigCategoryList": {
      "method": "GET",
      "type": "http",
      "url": "/api/frontCategories/childrenTree",
      "query": ["ids"]
    },
    "getArticleList": {
      "method": "GET",
      "type": "http",
      "url": "/api/article-center/list-latest-article",
      "query": ["pageNo", "pageSize"]
    },
    "getCategoryList": {
      "method": "GET",
      "type": "http",
      "url": "/api/article-center/list-article-category"
    }

  }
}
